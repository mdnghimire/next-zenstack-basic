    "@progress/kendo-react-spreadsheet": "^8.0.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.24.1",
    "@zenstackhq/runtime": "1.9.0",
    "@zenstackhq/server": "^1.9.0",
    "antd": "^5.17.4",
    "next": "^14.1.0",
    "next-auth": "^4.24.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
